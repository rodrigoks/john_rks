<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cnpjtransportadora">

    <insert id="insert" parameterType="CnpjTransportadoraVO" useGeneratedKeys="true" keyProperty="id" >

        INSERT INTO logbook06_cnpjs_transportadoras (
	        flogbook05_id,
	        flogbook06_cnpj
        ) VALUES
        <foreach collection="collection" item="item" index="index" open="(" separator="),(" close=")" >
            #{item.transportadora.id},
            #{item.cnpj}
        </foreach>

    </insert>

    <delete id="delete" parameterType="CnpjTransportadoraVO">
        DELETE FROM
        	logbook06_cnpjs_transportadoras
        WHERE
        	flogbook06_id = #{id};
    </delete>

    <select id="selectUnique" resultMap="result" parameterType="CnpjTransportadoraVO" >
        SELECT
        *
        FROM
        	logbook06_cnpjs_transportadoras
        WHERE
        	flogbook06_id = #{id};
    </select>

    <resultMap id="result" type="CnpjTransportadoraVO">
        <id property="id" column="flogbook06_id"/>
        <result property="cnpj" column="flogbook06_cnpj" />
    </resultMap>

</mapper>