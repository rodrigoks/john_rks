<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="logjust">

    <insert id="insert" parameterType="LogJustVO" useGeneratedKeys="true" keyProperty="id" >

        INSERT INTO logbook22_log_just (
            flogbook19_id,
            flogbook07_id,
            flogbook22_data_cadastro
        ) VALUES
        <foreach collection="collection" item="item" index="index" open="(" separator="),(" close=")" >
            #{item.justificativa.id},
            #{item.logbook.id},
            now()
        </foreach>

    </insert>

</mapper>