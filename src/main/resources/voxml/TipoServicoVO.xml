<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tiposervico">

    <select id="select" resultMap="result" >
        SELECT
          *
        FROM
          logbook12_tipo_servico
        ORDER BY
          logbook12_tipo_servico.flogbook12_id
        DESC;
    </select>

    <resultMap id="result" type="TipoServicoVO">
    	<id property="id" column="flogbook12_id"/>
        <result property="descricao" column="flogbook12_descricao" />
    </resultMap>

</mapper>