<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="arquivo">

    <select id="select" resultMap="result" parameterType="Arquivo" >
        SELECT
          *
        FROM
          logbook02_arquivos_importacao
        WHERE
          flogbook01_id = #{moduloId}
        ORDER BY
          flogbook02_nome_importacao
    </select>

    <resultMap id="result" type="Arquivo">
        <result property="id" column="flogbook02_id" />
        <result property="nomeImportacao" column="flogbook02_nome_importacao" />
        <result property="moduloId" column="flogbook01_id" />
    </resultMap>

</mapper>