<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tipocobranca">

    <select id="select" resultMap="result" >
        SELECT
          *
        FROM
          logbook13_tipo_cobranca
        ORDER BY
          logbook13_tipo_cobranca.flogbook13_id
        DESC;
    </select>

    <resultMap id="result" type="TipoCobrancaVO">
    	<id property="id" column="flogbook13_id"/>
        <result property="descricao" column="flogbook13_descricao" />
    </resultMap>

</mapper>