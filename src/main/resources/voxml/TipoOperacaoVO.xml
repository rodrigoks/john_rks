<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tipooperacao">

    <select id="select" resultMap="result" >
        SELECT
          *
        FROM
          logbook10_tipo_operacao
        ORDER BY
          logbook10_tipo_operacao.flogbook10_id
        DESC;
    </select>

    <resultMap id="result" type="TipoOperacaoVO">
    	<id property="id" column="flogbook10_id"/>
        <result property="descricao" column="flogbook10_descricao" />
    </resultMap>

</mapper>